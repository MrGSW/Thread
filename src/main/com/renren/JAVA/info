select 'website_info' ,'field:id', id,logo_url,bottom_logo_url,introduction_picture,background_picture,pre_video_picture,title_logo from website_info where dead_line > 1533739502000 and (logo_url is not null and logo_url <> '' ) or (bottom_logo_url is not null and bottom_logo_url <> '' ) or (introduction_picture is not null and introduction_picture <> '' ) or (background_picture is not null and background_picture <> '' ) or (pre_video_picture is not null and pre_video_picture <> '' ) or (title_logo is not null and title_logo <> '' ) limit 0, 8000;
select 'agent_team_member' ,'field:id', id,head_url from agent_team_member where (head_url is not null and head_url <> '' ) limit 0, 8000;

select 'agent_team' ,wi.domain_name,  agt.banner from agent_team agt inner join website_info wi on agt.site_id = wi.id and wi.dead_line > (unix_timestamp() * 1000) and (agt.banner is not null and agt.banner <> '' ) limit 0, 8000;
select 'agent_info' ,wi.domain_name, ai.head_url,ai.back_head_url,ai.photos from agent_info ai inner join website_info wi on ai.site_id = wi.id and wi.dead_line > (unix_timestamp()*1000) and ((ai.head_url is not null and ai.head_url <> '' ) or (ai.back_head_url is not null and ai.back_head_url <> '' ) or (ai.photos is not null and ai.photos <> '' ) ) limit 0, 8000;

